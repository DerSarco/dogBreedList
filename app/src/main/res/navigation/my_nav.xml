<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/dogBreedListFragment">

    <fragment
        android:id="@+id/dogBreedListFragment"
        android:name="com.sarco.dogbreed.ui.fragments.breedList.BreedListFragment"
        android:label="fragment_dog_breed_list"
        tools:layout="@layout/fragment_dog_breed_list" >
        <action
            android:id="@+id/action_dogBreedListFragment_to_breedDetailListFragment"
            app:destination="@id/breedDetailListFragment" />
        <action
            android:id="@+id/action_dogBreedListFragment_to_breedDataFavoritesFragment"
            app:destination="@id/breedDataFavoritesFragment" />
    </fragment>
    <fragment
        android:id="@+id/breedDetailListFragment"
        android:name="com.sarco.dogbreed.ui.fragments.bredDetailList.BreedDetailListFragment"
        android:label="fragment_breed_detail_list"
        tools:layout="@layout/fragment_breed_detail_list" />
    <fragment
        android:id="@+id/breedDataFavoritesFragment"
        android:name="com.sarco.dogbreed.ui.fragments.breedDataFavorites.BreedDataFavoritesFragment"
        android:label="fragment_breed_data_favorites"
        tools:layout="@layout/fragment_breed_data_favorites" />
</navigation>